{
  "name": "@dzangolab/react-i18n",
  "version": "0.1.0",
  "description": "React I18n Plugin",
  "exports": {
    ".": {
      "import": "./dist/DzangolabReactI18n.es.js",
      "require": "./dist/DzangolabReactI18n.umd.js"
    },
    "./dist/DzangolabReactI18n.css": "./dist/style.css"
  },
  "main": "dist//DzangolabReactI18n.umd.js",
  "module": "dist//DzangolabReactI18n.es.js",
  "types": "./dist/src/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "vitest --environment jsdom run --coverage"
  },
  "dependencies": {
    "i18next": "21.8.13",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-chained-backend": "^4.0.0",
    "i18next-resources-to-backend": "^1.0.0",
    "react-i18next": "11.18.6",
    "react-outside-click-handler": "1.3.0"
  },
  "devDependencies": {
    "@dzangolab/react-config": "0.1.0",
    "@testing-library/react": "13.4.0",
    "@types/i18n": "0.13.4",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-outside-click-handler": "1.3.1",
    "@types/react-test-renderer": "18.0.0",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-c8": "0.24.1",
    "eslint": "8.25.0",
    "eslint-config-custom": "0.1.0",
    "eslint-plugin-react": "7.31.10",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "tsconfig": "0.1.0",
    "typescript": "4.8.4",
    "vite": "3.1.7",
    "vitest": "0.25.8"
  },
  "peerDependencies": {
    "@dzangolab/react-config": "0.1.0",
    "react": ">=18.2",
    "react-dom": ">=18.2"
  }
}
