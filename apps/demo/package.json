{
  "name": "@dzangolab/demo-app",
  "homepage": "https://dzangolab.github.io/react",
  "version": "0.6.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "snapshot:update": "vitest --environment jsdom run --update",
    "preview": "vite preview",
    "sort-package": "npx sort-package-json",
    "test": "CI=true vitest",
    "test:unit": "CI=true vitest",
    "typecheck": "tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@dzangolab/react-config": "workspace:*",
    "@dzangolab/react-form": "workspace:*",
    "@dzangolab/react-i18n": "workspace:*",
    "@dzangolab/react-layout": "workspace:*",
    "@dzangolab/react-ui": "workspace:*",
    "@dzangolab/react-user": "workspace:*",
    "@reduxjs/toolkit": "1.9.5",
    "normalize.css": "8.0.1",
    "primeicons": "6.0.1",
    "primereact": "9.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.15.0",
    "react-toastify": "9.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/plugin-syntax-flow": "7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@testing-library/react": "14.0.0",
    "@types/jsdom": "21.1.4",
    "@types/node": "18.17.12",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@types/react-test-renderer": "18.0.5",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-c8": "0.30.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "44.0.2",
    "jsdom": "22.1.0",
    "prettier": "3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.1.6",
    "vite": "4.5.2",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "3.2.0",
    "vitest": "0.30.1"
  }
